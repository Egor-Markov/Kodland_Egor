import discord
import random
from discord.ext import commands
import time

token = ""


intents = discord.Intents.default()
intents.message_content = True

bot = commands.Bot(command_prefix='$', intents=intents)

@bot.event
async def on_ready():
    print(f'We have logged in as {bot.user}')

@bot.command()
async def hello(ctx):
    await ctx.send('–ó–¥—Ä–∞–≤—Å—Ç–≤—É–π—Ç–µ!–Ø —ç–∫–æ-–±–æ—Ç, –µ—Å–ª–∏ –í–∞–º –±–æ–ª—å—à–µ 20-—Ç–∏ –ª–µ—Ç, –í–∞—Å –±–µ—Å–ø–æ–∫–æ–∏—Ç —ç–∫–æ–ª–æ–≥–∏—è –∏ —Ç–æ, –∫–∞–∫ –µ—ë —É–ª—É—á—à–∏—Ç—å - –í—ã –ø–æ–ø–∞–ª–∏ –ø–æ –∞–¥—Ä–µ—Å—É!')
    time.sleep(4)
    await ctx.send('–ß—Ç–æ –∂, –í—ã –∑–∞–∏–Ω—Ç–µ—Ä–µ—Å–æ–≤–∞–Ω—ã, –¥–∞–≤–∞–π—Ç–µ –≤—ã–±–µ—Ä–µ–º –∏–Ω—Ç–µ—Ä–µ—Å—ã: 1)–ß—Ç–æ –∑–∞–≥—Ä—è–∑–Ω—è–µ—Ç—Å—è? 2)–ö–∞–∫ –ø–æ–º–æ—á—å –ø—Ä–∏—Ä–æ–¥–µ? (–û—Ç–≤–µ—Ç—å—Ç–µ: $choice_1 –∏–ª–∏ $choice_2)')

@bot.command()
async def choice_1(ctx):
    await ctx.send('–ü—Ä–∏—Ä–æ–¥—É –∑–∞–≥—Ä—è–∑–Ω—è—é—Ç –≤–æ –º–Ω–æ–≥–∏—Ö –º–µ—Ç–∞—Ö: –≤–æ–¥–∞, –≤–æ–∑–¥—É—Ö –∏–ª–∏ –ø–æ—á–≤–∞ - –∫–∞–∫ –∂–µ —ç—Ç–æ –æ—Å—Ç–∞–Ω–æ–≤–∏—Ç—å? –ü–∏—à–∏—Ç–µ: $protection')
    time.sleep(2)
    await ctx.send('üåé')

@bot.command()
async def choice_2(ctx):
    await ctx.send('–ï—Å—Ç—å –æ—á–µ–Ω—å –º–Ω–æ–≥–æ —Å–ø–æ—Å–æ–±–æ–≤ –ø–æ–º–æ—â–∏ –Ω–∞—à–µ–π –ø–ª–∞–Ω–µ—Ç–µ, –µ—Å–ª–∏ –∫–∞–∂–¥—ã–π —á–µ–ª–æ–≤–µ–∫ —Å–¥–µ–ª–∞–µ—Ç –ø–æ-–Ω–µ–º–Ω–æ–≥—É, —Ç–æ –ó–µ–º–ª–µ –∏ –ø—Ä–∏—Ä–æ–¥–µ —É–∂–µ —Å—Ç–∞–Ω–µ—Ç –ª–µ–≥—á–µ. –í–æ—Ç –∫–∞–∫–∏–µ –≤–∞—Ä–∏–∞–Ω—Ç—ã –∏ —Å–æ–≤–µ—Ç—ã —è –º–æ–≥—É –¥–∞—Ç—å: 1)–°–∞–∂–∞—Ç—å –¥–µ—Ä–µ–≤—å—è($planting); 2)–°–æ–∫—Ä–∞—Ç–∏—Ç—å –≤—ã–±—Ä–æ—Å—ã —É–≥–ª–µ–∫–∏—Å–ª–æ–≥–æ –≥–∞–∑–∞($CO); 3)–°–æ—Ä—Ç–∏—Ä–æ–≤–∞—Ç—å –º—É—Å–æ—Ä($sorting); 4)–ß–∏—Å—Ç—ã–µ –ø–æ—Ö–æ–¥—ã –∏ –≥—É–ª—è–Ω–∫–∏($camping)')
    time.sleep(2)
    await ctx.send('üåç üëå')

@bot.command()
async def planting(ctx):
    await ctx.send('–û–¥–Ω–æ –≤–∑—Ä–æ—Å–ª–æ–µ –¥–µ—Ä–µ–≤–æ –≤—ã–¥–µ–ª—è–µ—Ç –≤ —Å—Ä–µ–¥–Ω–µ–º 120 –∫–≥ –∫–∏—Å–ª–æ—Ä–æ–¥–∞ –≤ –≥–æ–¥. –≠—Ç–æ–≥–æ –¥–æ—Å—Ç–∞—Ç–æ—á–Ω–æ –¥–ª—è —Å–µ–º—å–∏ –∏–∑ —Ç—Ä–µ—Ö —á–µ–ª–æ–≤–µ–∫. –ù–æ, –∫ —Å–æ–∂–∞–ª–µ–Ω–∏—é, –≤–∑—è—Ç—å –∏ –ø–æ—Å–∞–¥–∏—Ç—å –¥–µ—Ä–µ–≤–æ –≤–æ –¥–≤–æ—Ä–µ –º–Ω–æ–≥–æ–∫–≤–∞—Ä—Ç–∏—Ä–Ω–æ–≥–æ –¥–æ–º–∞ –Ω–µ–ª—å–∑—è - –º–æ–∂–Ω–æ –ª–∏—à—å –≤—ã—Å—Ç—É–ø–∏—Ç—å —Å –∏–Ω–∏—Ü–∏–∞—Ç–∏–≤–æ–π –∏ –∂–¥–∞—Ç—å –∑–∞–∫–æ–Ω–Ω–æ–≥–æ —Ä–µ—à–µ–Ω–∏—è. –î–∞ –∏ –≤ —á–∞—Å—Ç–Ω–æ–º —Å–µ–∫—Ç–æ—Ä–µ —Å–≤–æ–∏ –æ–≥—Ä–∞–Ω–∏—á–µ–Ω–∏—è. –ì–æ—Ä–∞–∑–¥–æ —ç—Ñ—Ñ–µ–∫—Ç–∏–≤–Ω–µ–µ –æ–∑–µ–ª–µ–Ω–µ–Ω–∏–µ –ø–ª–∞–Ω–µ—Ç—ã –ø—Ä–æ–∏—Å—Ö–æ–¥–∏—Ç, –µ—Å–ª–∏ –æ–±—ä–µ–¥–∏–Ω–∏—Ç—å —É—Å–∏–ª–∏—è. –°—Ö–æ–¥–∏—Ç—å –Ω–∞ —Å—É–±–±–æ—Ç–Ω–∏–∫ –≤ –ø–∞—Ä–∫ –∏–ª–∏ –ø—Ä–∏–º–∫–Ω—É—Ç—å –∫ –æ—Ä–≥–∞–Ω–∏–∑–∞—Ü–∏–∏ –ø–æ –≤–æ—Å—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω–∏—é –ª–µ—Å–æ–≤.')

@bot.command()
async def CO(ctx):
    await ctx.send('–ù–∞ –¥–æ–ª—é –∞–≤—Ç–æ–º–æ–±–∏–ª–µ–π –ø—Ä–∏—Ö–æ–¥–∏—Ç—Å—è –¥–æ 80% –∑–∞–≥—Ä—è–∑–Ω–µ–Ω–∏—è –≤–æ–∑–¥—É—Ö–∞ –≤ –≥–æ—Ä–æ–¥–∞—Ö. –ü–æ—ç—Ç–æ–º—É —Ç–∞–∫–∏–µ –ø—Ä–æ—Å—Ç—ã–µ —à–∞–≥–∏, –∫–∞–∫ –∏—Å–ø–æ–ª—å–∑–æ–≤–∞–Ω–∏–µ –æ–±—â–µ—Å—Ç–≤–µ–Ω–Ω–æ–≥–æ —Ç—Ä–∞–Ω—Å–ø–æ—Ä—Ç–∞ –∏ —Å–µ—Ä–≤–∏—Å–æ–≤ —Å–æ–≤–º–µ—Å—Ç–Ω—ã—Ö –ø–æ–µ–∑–¥–æ–∫ —É–º–µ–Ω—å—à–∞—é—Ç –≤—ã–±—Ä–æ—Å—ã –°–û2 –≤ –∞—Ç–º–æ—Å—Ñ–µ—Ä—É. –ê –ø–µ—Ä–µ—Å–∞–¥–∫–∞ –Ω–∞ –≤–µ–ª–æ—Å–∏–ø–µ–¥ –∏ –ø–µ—à–∏–µ –ø—Ä–æ–≥—É–ª–∫–∏ –µ—â–µ –∏ –¥–ª—è –∑–¥–æ—Ä–æ–≤—å—è –ø–æ–ª–µ–∑–Ω—ã. –£–º–µ–Ω—å—à–∞—è –º–æ—â–Ω–æ—Å—Ç—å –æ—Ç–æ–ø–∏—Ç–µ–ª—å–Ω—ã—Ö –ø—Ä–∏–±–æ—Ä–æ–≤, –º—ã –Ω–µ —Ç–æ–ª—å–∫–æ —Å–Ω–∏–∂–∞–µ–º –∫–æ–ª–∏—á–µ—Å—Ç–≤–æ –≤—Ä–µ–¥–Ω—ã—Ö –≤—ã–±—Ä–æ—Å–æ–≤, –Ω–æ –∏ —ç–∫–æ–Ω–æ–º–∏–º —Å–µ–º–µ–π–Ω—ã–π –±—é–¥–∂–µ—Ç. –ü–æ–º–æ–≥–∞—é—Ç —Å–æ—Ö—Ä–∞–Ω–∏—Ç—å –¥–æ–º–∞ –ø—Ä–∏—è—Ç–Ω—ã–π –º–∏–∫—Ä–æ–∫–ª–∏–º–∞—Ç –∫–æ—Ç–ª—ã —Å –≤—ã—Å–æ–∫–∏–º –ö–ü–î –∏ —É—Ç–µ–ø–ª–µ–Ω–∏–µ –Ω–∞—Ä—É–∂–Ω—ã—Ö —Å—Ç–µ–Ω.')

@bot.command()
async def sorting(ctx):
    await ctx.send('–ù–µ—Å–∫–æ–ª—å–∫–æ –ø—Ä–æ—Å—Ç—ã—Ö –ø—Ä–∞–≤–∏–ª –ø–æ —Å–æ—Ä—Ç–∏—Ä–æ–≤–∫–µ –º—É—Å–æ—Ä–∞ –ø–æ–º–æ–≥—É—Ç –Ω–µ –æ—Å—Ç–∞–Ω–æ–≤–∏—Ç—å—Å—è –≤ —Å–∞–º–æ–º –Ω–∞—á–∞–ª–µ —ç–∫–æ–ª–æ–≥–∏—á–µ—Å–∫–æ–≥–æ –ø—É—Ç–∏. –ü–æ—Å—Ç–∞—Ä–∞–π—Ç–µ—Å—å –¥–ª—è –Ω–∞—á–∞–ª–∞ —É–º–µ–Ω—å—à–∏—Ç—å –∫–æ–ª–∏—á–µ—Å—Ç–≤–æ –º—É—Å–æ—Ä–∞. –•–æ–¥–∏—Ç–µ –≤ –º–∞–≥–∞–∑–∏–Ω —Å —Ç–∫–∞–Ω–µ–≤–æ–π —Å—É–º–∫–æ–π, –ø—Ä–æ–¥–∞–≤–∞–π—Ç–µ –∏ —Ä–∞–∑–¥–∞–≤–∞–π—Ç–µ –Ω–µ–Ω—É–∂–Ω—É—é –æ–¥–µ–∂–¥—É –∏ –æ–±—É–≤—å. –ü–æ—Å–µ—Ç–∏—Ç–µ –Ω–∞—à—É —Ç—É—Ä–∏—Å—Ç–∏—á–µ—Å–∫—É—é –±–∞—Ä–∞—Ö–æ–ª–∫—É, —á—Ç–æ–±—ã –Ω–∞–π—Ç–∏ –≤—Ç–æ—Ä—ã–µ —Ä—É–∫–∏ –¥–ª—è –ª–∏—à–Ω–µ–≥–æ —Å–Ω–∞—Ä—è–∂–µ–Ω–∏—è –∏–ª–∏ –≤—ã–≥–æ–¥–Ω–æ –ø—Ä–∏–æ–±—Ä–µ—Å—Ç–∏ –Ω—É–∂–Ω–æ–µ. –ï—Å–ª–∏ —Ä–µ—à–∏–ª–∏ —Å–æ—Ä—Ç–∏—Ä–æ–≤–∞—Ç—å –º—É—Å–æ—Ä, –Ω–∞—á–Ω–∏—Ç–µ —Å —Å–∞–º–æ–≥–æ –ø—Ä–æ—Å—Ç–æ–≥–æ, –Ω–∞–ø—Ä–∏–º–µ—Ä, —Å–æ–±–∏—Ä–∞–π—Ç–µ –ø–ª–∞—Å—Ç–∏–∫–æ–≤—ã–µ –±—É—Ç—ã–ª–∫–∏ –∏ –±–∞—Ç–∞—Ä–µ–π–∫–∏. –ü—Ä–∏—Å–º–æ—Ç—Ä–∏—Ç–µ—Å—å –∫ –æ–±–æ–∑–Ω–∞—á–µ–Ω–∏—é –Ω–∞ –ø–ª–∞—Å—Ç–∏–∫–µ - –Ω–µ –≤—Å–µ –µ–≥–æ –≤–∏–¥—ã –ø–æ–¥–ª–µ–∂–∞—Ç –ø–µ—Ä–µ—Ä–∞–±–æ—Ç–∫–µ. –û–± —ç—Ç–æ–º –º—ã –ø–æ–¥—Ä–æ–±–Ω–æ —Ä–∞—Å—Å–∫–∞–∑—ã–≤–∞–ª–∏ –≤ —Å—Ç–∞—Ç—å–µ –ö—É–¥–∞ –¥–µ–≤–∞—Ç—å –º—É—Å–æ—Ä –≤ –ø–æ—Ö–æ–¥–µ. –ü–ª–∞—Å—Ç–∏–∫–æ–≤—ã–µ –±—É—Ç—ã–ª–∫–∏ –Ω—É–∂–Ω–æ —Å–ø–æ–ª–æ—Å–Ω—É—Ç—å, –æ—á–∏—Å—Ç–∏—Ç—å –æ—Ç —Ç–µ—Ä–º–æ—É—Å–∞–¥–æ—á–Ω–æ–π –ø–ª–µ–Ω–∫–∏, —Å–Ω—è—Ç—å –∫—Ä—ã—à–µ—á–∫–∏ –∏ —Å–ø—Ä–µ—Å—Å–æ–≤–∞—Ç—å. –ë—É–º–∞–≥—É, –∫–∞—Ä—Ç–æ–Ω, —Å–ª–æ–∂–∏—Ç–µ –≤ –∫–∏–ø—É –∏ –ø–µ—Ä–µ–≤—è–∂–∏—Ç–µ –≤–µ—Ä–µ–≤–∫–æ–π. –ú–µ—Ç–∞–ª–ª–∏—á–µ—Å–∫–∏–µ –∏ –ø–ª–∞—Å—Ç–∏–∫–æ–≤—ã–µ —ç–ª–µ–º–µ–Ω—Ç—ã, –Ω–∞–ø—Ä–∏–º–µ—Ä, –∑–∞–∂–∏–º—ã —Å –ø–∞–ø–æ–∫, –Ω—É–∂–Ω–æ –æ—Ç–¥–µ–ª–∏—Ç—å. –ú–µ—Ç–∞–ª–ª –Ω—É–∂–Ω–æ –æ—á–∏—Å—Ç–∏—Ç—å –æ—Ç –æ—Å—Ç–∞—Ç–∫–æ–≤ –ø–∏—â–∏ –∏ —ç—Ç–∏–∫–µ—Ç–∫–∏, –±–∞–Ω–∫–∏ —Å–ø–ª—é—â–∏—Ç—å.')

@bot.command()
async def camping(ctx):
    await ctx.send('–≠–∫–æ–ª–æ–≥–∏—á–µ—Å–∫–∏–π –∫–æ–¥–µ–∫—Å —Ç—É—Ä–∏—Å—Ç–∞: 1. –û—Å—Ç–∞–≤—å —Å—Ç–æ—è–Ω–∫—É —á–∏—â–µ, —á–µ–º –±—ã–ª–∞; 2. –ë–µ—Ä–µ–∂–Ω–æ –æ—Ç–Ω–æ—Å–∏—Å—å –∫ –ø—Ä–∏—Ä–æ–¥–µ; 3. –ó–∞—Ç—É—à–∏ –∫–æ—Å—Ç–µ—Ä.')

bot.run(token)

